model Goal {
    id        Int       @id @default(autoincrement())
    childId   Int
    title     String    @db.VarChar(50)
    points    Int?
    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    deletedAt DateTime?

    child Child @relation(fields: [childId], references: [id])

    @@unique([childId, title])
}
